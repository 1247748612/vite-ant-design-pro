    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "magic-string": "^0.26.1",
    "pnpm": "^7.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
